<h1>Listing completed_courses</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Section</th>
      <th>Term</th>
      <th>User</th>
      <th>Grade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @completed_courses.each do |completed_course| %>
      <tr>
        <td><%= completed_course.course_id %></td>
        <td><%= completed_course.section_id %></td>
        <td><%= completed_course.term %></td>
        <td><%= completed_course.user_id %></td>
        <td><%= completed_course.grade %></td>
        <td><%= link_to 'Show', completed_course %></td>
        <td><%= link_to 'Edit', edit_completed_course_path(completed_course) %></td>
        <td><%= link_to 'Destroy', completed_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Completed course', new_completed_course_path %>
