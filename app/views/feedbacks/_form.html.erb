<%= form_for(@feedback) do |f| %>
  <% if @feedback.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@feedback.errors.count, "error") %> prohibited this feedback from being saved:</h2>

      <ul>
      <% @feedback.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.hidden_field :user_id, value: current_user.id %>
  </div>
  <div class="field">
    <%= f.label :feedback_category %><br>
    <%= f.select :feedback_type, ["Suggestion", "Flag or Report", "Bug", "Request to become an Ambassador"], {prompt: "Provide a category this feedback belongs to"}, {class: "input-large form-control"} %>
  </div>
  <div class="field">
    <%= f.label :write_your_feedback_here %>:<br>
    <%= f.text_area :content, rows: 4, class: 'form-control' %>
  </div>
  <div class="actions">
    <%= f.submit class: 'btn btn-primary' %>
  </div>
<% end %>
